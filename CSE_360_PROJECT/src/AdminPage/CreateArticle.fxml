<?xml version="1.0" encoding="UTF-8"?>
<!-- Import necessary JavaFX controls -->
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:controller="Article.ArticleController" prefHeight="520.0" prefWidth="732.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="233.0" layoutY="28.0" prefHeight="51.0" prefWidth="266.0" text="Setup an article" textFill="#030303">
         <font>
            <Font size="28.0" /> 
         </font>
      </Label>

      <Label fx:id="usernameLabel" layoutX="38.0" layoutY="89.0" prefHeight="17.0" prefWidth="57.0" text="Header: " textFill="#000000">
         <font>
            <Font size="15.0" /> 
         </font>
      </Label>

      <Label fx:id="usernameLabel1" layoutX="198.0" layoutY="89.0" prefHeight="17.0" prefWidth="109.0" text="Title Of Article:" textFill="#000000">
         <font>
            <Font size="15.0" />
         </font>
      </Label>

      <TextField fx:id="Username_textField" layoutX="38.0" layoutY="110.0" promptText="Header" />
      <TextField fx:id="Username_textField1" layoutX="198.0" layoutY="110.0" prefHeight="25.0" prefWidth="149.0" promptText="Title" />

      <Label fx:id="usernameLabel11" layoutX="35.0" layoutY="159.0" prefHeight="27.0" prefWidth="149.0" text="Short Description:" textFill="#000000">
         <font>
            <Font size="18.0" />
         </font>
      </Label>

      <Label fx:id="usernameLabel12" layoutX="390.0" layoutY="86.0" prefHeight="27.0" prefWidth="109.0" text="Keywords:" textFill="#000000">
         <font>
            <Font size="15.0" />
         </font>
      </Label>

      <Label fx:id="usernameLabel13" layoutX="33.0" layoutY="274.0" prefHeight="17.0" prefWidth="109.0" text="Body of Article:" textFill="#000000">
         <font>
            <Font size="16.0" />
         </font>
      </Label>

      <Label fx:id="usernameLabel14" layoutX="33.0" layoutY="386.0" prefHeight="17.0" prefWidth="109.0" text="Links:" textFill="#000000">
         <font>
            <Font size="16.0" />
         </font>
      </Label>

      <TextField fx:id="Username_textField11" layoutX="390.0" layoutY="110.0" prefHeight="25.0" prefWidth="149.0" promptText="(i.e Grades, Attendance) " />

      <!-- Added fx:id for shortDescription, bodyOfArticle, links, and sensitiveInfo -->
      <TextArea fx:id="shortDescription" layoutX="36.0" layoutY="186.0" prefHeight="70.0" prefWidth="232.0" promptText="Chris's grades are not good" />
      <TextArea fx:id="bodyOfArticle" layoutX="34.0" layoutY="299.0" prefHeight="70.0" prefWidth="416.0" promptText="Don't make this long cause we dont have enough space" />
      <TextArea fx:id="links" layoutX="30.0" layoutY="411.0" prefHeight="51.0" prefWidth="416.0" promptText="google.com" />

      <Label fx:id="usernameLabel111" layoutX="499.0" layoutY="159.0" prefHeight="27.0" prefWidth="175.0" text="Sensitive Information" textFill="#000000">
         <font>
            <Font size="18.0" />
         </font>
      </Label>

      <TextArea fx:id="sensitiveInfo" layoutX="493.0" layoutY="187.0" prefHeight="200.0" prefWidth="200.0" promptText="Don't look in here" />

      <Button layoutX="473.0" layoutY="424.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="207.0" text="Finish Article" onAction="#handleFinishArticle" />
   </children>
</AnchorPane>
